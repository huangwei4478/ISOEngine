[system]

; #pragma bank <bank> for rooms
bank=3

; modifier for rooms (alternative banking)
;modifier=CODE_3

; list of #include
includes=nonintrinsic.h,scenes.h

; 3D Scene dimentions (don't edit unless you know what are you doing)
XYZ=9,9,4

; vocabulary: index of the character is an id of an element
voc=0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ

; name of the list in the source
rooms=r0,r1,r2,r3,r4,r5,r6,r7,r8,r9,rA

; name of the section which contains the global world map
world=world

[world]
; number of rows
rows=4

; modifier for world array (alternative banking)
modifier=CODE_1

; 2D global room map, each char is an ID of the room, unknown id means NULL room

;      W
;      ^
;      |
; S <-- --> N
;      |
;      v
;      E

0=01..
1=2345
2=.A.6
3=.987

[r0]
; id in the global world map (single char)
id=0
; list of section names that contain 2D slices
layers=r0l0,r0l1,r0l2,r0l3

